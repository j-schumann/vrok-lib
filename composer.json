{
    "name": "vrok/vrok-lib",
    "description": "Vrok Library",
    "type": "library",
    "license": "MIT",
    "keywords": ["zf2", "doctrine", "auth"],
    "homepage": "http://www.vrok.de/",
    "minimum-stability": "dev",

    "require": {
        "php": ">=5.6",
        "doctrine/doctrine-orm-module": "1.1.0",
        "gedmo/doctrine-extensions": "^3.0.0",
        "ircmaxell/random-lib": "~1.1.0",
        "ocramius/proxy-manager": "~1.0.2",
        "rwoverdijk/assetmanager": "^1.7.1",
        "zendframework/zend-component-installer": "^0.3.1",
        "zendframework/zend-filter": "^2.7.1",
        "zendframework/zend-hydrator": "^2.2.1",
        "zendframework/zend-inputfilter": "^2.7.3",
        "zendframework/zend-mail": "^2.6.1",
        "zendframework/zend-mvc": "^3.0.3",
        "zendframework/zend-mvc-console": "^1.1.11",
        "zendframework/zend-mvc-form": "^1.0.0",
        "zendframework/zend-mvc-i18n": "^1.0.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0.0",
        "zendframework/zend-mvc-plugin-identity": "^1.0.0",
        "zendframework/zend-validator": "^2.8.1"
    },
    "suggest": {
        "ext-intl": "Required for translator & IDNA convert"
    },
    "autoload": {
        "psr-4": {
            "Vrok\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-dev": "3.0.x",
            "dev-master": "2.0.x"
        }
    }
}