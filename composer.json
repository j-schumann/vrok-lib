{
    "name": "vrok/vrok-lib",
    "description": "Vrok Library",
    "type": "library",
    "license": "MIT",
    "keywords": ["zf2", "doctrine", "auth"],
    "homepage": "http://www.vrok.de/",
    "minimum-stability": "dev",

    "require": {
