        "doctrine/doctrine-orm-module": "~0.10.0",
        "gedmo/doctrine-extensions": "~2.4.13",
        "ircmaxell/random-lib": "~1.1.0",
        "ocramius/proxy-manager": "~1.0.2",
        "rwoverdijk/assetmanager": "~1.5.1",
        "zendframework/zend-component-installer": "^0.3.1",
        "zendframework/zend-filter": "^2.5.3",
        "zendframework/zend-hydrator": "^2.5.3",
        "zendframework/zend-inputfilter": "^2.5.3",
        "zendframework/zend-mail": "^2.5.3",
        "zendframework/zend-mvc": "^2.7.0",
        "zendframework/zend-mvc-console": "^2.5.3",
        "zendframework/zend-mvc-form": "^2.5.3",
        "zendframework/zend-mvc-i18n": "^2.5.3",
        "zendframework/zend-mvc-plugins": "^2.5.3",
        "zendframework/zend-validator": "^2.5.3"
    },
    "suggest": {
        "ext-intl": "Required for translator & IDNA convert"
    },
    "autoload": {
        "psr-4": {
            "Vrok\\": "src/"
        }
    }
